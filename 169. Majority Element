class Solution {
public:
    int majorityElement(vector<int>& nums){
         int n=nums.size();
         int a =n/2;
         unordered_map<int,int>v;
         for(int i : nums){
            v[i]++;
            if(v[i]>a){
                return i;
            
            }
         }   
     return -1;
    }
};
